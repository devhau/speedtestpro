{
  "name": "speedtestpro",
  "version": "1.0.0-beta",
  "description": "**speedtest** is a Javascript library for non-blocking notifications and without jQuery. The goal is to create a simple core library that can be customized and extended.",
  "main": "dist/speedtest.js",
  "module": "dist/speedtest.esm.js",
  "browser": "dist/speedtest.iife.js",
  "types": "dist/types/speedtest.d.ts",
  "files": [
    "dist/*"
  ],
  "bin": {
    "stp": "./bin/index.js"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "rimraf dist && npm run build:types && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devhau/speedtest.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devhau/speedtest/issues"
  },
  "homepage": "https://github.com/devhau/speedtest#readme",
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-sucrase": "^5.0.1",
    "rimraf": "^4.1.2",
    "rollup": "2.78.0",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.58.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
